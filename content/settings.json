      "medium": "https://konstantinmuenster.medium.com",
      "linkedin": "https://www.linkedin.com/in/tim-kelley-456112b9/",
      "mail": "mailto:timkelley683@gmail.com"
    }
  },
  "siteConfiguration": {
    "logo": { "text": "My Portfolio" },
    "navigation": {
      "header": [
        { "label": "About", "url": "/#about" },
        { "label": "Blog", "url": "/blog" },
        { "label": "Projects", "url": "/#features" },
        { "label": "Contact", "url": "/#github" }
